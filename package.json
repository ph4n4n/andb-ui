{
  "name": "andb",
  "productName": "Andb",
  "version": "1.0.0",
  "description": "Simplest Database Migration Tool by ph4n4n",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "build:electron": "tsc -p tsconfig.electron.json",
    "electron:dev": "npm run build:electron && NODE_ENV=development concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && npm run build:electron && electron-builder",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .eslintignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore",
    "format": "prettier --write \"src/**/*.{js,ts,vue,css,scss,json}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "sample:load": "node scripts/load-sample-data.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ph4n4n/andb-ui.git"
  },
  "keywords": [
    "electron",
    "vue",
    "database",
    "migration",
    "andb-core"
  ],
  "author": "ph4n4n",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/ph4n4n/andb-ui/issues"
  },
  "homepage": "https://github.com/ph4n4n/andb-ui#readme",
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.1.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.19.10",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-vue": "^4.5.2",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.0",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "electron-rebuild": "^3.2.9",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.1",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vue-tsc": "^3.0.5",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "@andb/core": "file:../andb-core",
    "@headlessui/vue": "^1.7.23",
    "@types/prismjs": "^1.26.5",
    "@vueuse/core": "^10.7.1",
    "andb-logger": "^1.0.3",
    "better-sqlite3": "^12.4.1",
    "electron-store": "^8.2.0",
    "lucide-vue-next": "^0.294.0",
    "mysql2": "^3.15.3",
    "pinia": "^2.1.7",
    "prismjs": "^1.30.0",
    "vue": "^3.4.0",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.2.5",
    "vuedraggable": "^4.1.0"
  }
}
